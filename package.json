{
  "private": true,
  "name": "opportfolio",
  "version": "1.2.0",
  "homepage": "https://www.aryankr.in/",
  "scripts": {
    "prebuild": "node generate-sitemap.js",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "eject": "react-scripts eject",
    "generate:sitemap": "node generate-sitemap.js",
    "prepare": "husky install",
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "glamor": "^2.20.40",
    "react": "^18.3.1",
    "react-awesome-reveal": "^4.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-gtm-module": "^2.0.11",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.3.8"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "prettier-package-json": "^2.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.json": "prettier-package-json --write",
    "*.{js,css,md}": "prettier --write"
  },
  "overrides": {
    "eslint": ">=9.26.0",
    "jsonpath": "npm:jsonpath-plus@^10.0.0",
    "qs": ">=6.14.2"
  }
